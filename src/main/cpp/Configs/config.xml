<?xml version="1.0" encoding="UTF-8"?>
<!-- Root of the Config -->
<Robot>
    <!-- Version of the current config to be printed on the console -->
    <Version version="1" />
    <!-- Comment on this particular config to be printed on the console -->
    <Comment comment="A" />
    <!-- Sets whether or not to initialize the NavX -->
    <UseNavX value="true" />
    <!-- Sets whether or not to initialize the camera to send back to the driver station -->
    <!-- Contains assignments to ports and robot dimensions -->
    <RobotConfig>
        <!-- Region for defining VictorSP Items -->
        <VictorSP>
            <intake channel="8" reversed="false"/>
        </VictorSP>
        <!-- Region for VictorSPX CAN Items -->
        <VictorSPX>
            <left_0 channel="0" reversed="false"/>
            <left_1 channel="1" reversed="false"/>
            <right_0 channel="2" reversed="false"/>
            <right_1 channel="3" reversed="false"/> <!-- CHECK TGE REVERSE-->
            <elevator_0 channel="4" reversed="false"/>
            <elevator_1 channel="5" reversed="false"/>
            <elevator_2 channel="6" reversed="false"/>
            <elevator_3 channel="7" reversef="false"/>
        </VictorSPX>
        <!-- Region for TalonSRX CAN Items -->
        <TalonSRX>
        </TalonSRX>
        <!-- Region for Potentiometers -->
        <Potentiometer>
            <elevatorPot channel="0"/>
        </Potentiometer>
        <!-- Region for Encoders -->
        <Encoder>
            <enc0 aChannel="0" bChannel="1"/>
            <enc1 aChannel="2", bChannel="3"/>
        </Encoder>
        <!-- Region for DoubleSolenoids -->
        <DoubleSolenoid>
        </DoubleSolenoid>
        <!-- Region for DigitalInputs -->
        <DigitalInput>
        </DigitalInput>
    </RobotConfig>

    <!-- Root of all control definitions -->
    <Controls>
        <!-- Driver Controller, slot should always be 0 -->
        <Driver slot="0">
            <AxisControls>
                <leftDrive axis="1" deadZone="0.08" reversed="false" powerMultiplier="0.8" bindings="left_0, left_1"/>
                <rightDrive axis="5" deadZone="0.08" reversed="false" powerMultiplier="0.8" bindings="right_0, right_1"/>
            </AxisControls>
        </Driver>
        <Operator slot="1">
            <AxisControls>
                <elevatorAxis axis="1" deadZone="0.08" reversed="false" powerMultiplier="0.8" binding="elevator_0, elevator_1, elevator_2, elevator_3"/>
                <intakeAxis axis="5" deadZone="0.08" reversed="false" powerMultiplier="0.8" binding="intake"/>
            </AxisControls>
        </Operator>
    </Controls>
</Robot>